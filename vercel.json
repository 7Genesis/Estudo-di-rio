{
  "version": 2,
  "builds": [
    {
      "src": "app-juridico/react-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app-juridico/react-frontend/build/$1"
    }
  ]
}
